<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learndeno">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ZXing2Activity"></activity>
        <activity
            android:name=".activity.ZBarActivity"
            android:label="@string/zbar_name" />
        <activity
            android:name=".activity.ZXingActivity"
            android:label="@string/zxing_name" />

        <service
            android:name=".service.MusicService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".activity.ServiceActivity" />
        <activity
            android:name=".activity.HandleThreadActivity"
            android:label="@string/handle_Thread_name" />
        <activity
            android:name=".activity.LayoutActivity"
            android:label="@string/layout_name" />
        <activity
            android:name=".activity.CanvasActivity"
            android:label="@string/canvas_name" />
        <activity
            android:name=".activity.DialogActivity"
            android:label="@string/dialog_name" />
        <activity
            android:name=".activity.TextShowActivity"
            android:label="@string/light_name" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>